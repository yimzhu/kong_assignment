{
  "name": "kong_assignment",
  "version": "1.0.0",
  "description": "automation",
  "main": "index.js",
  "scripts": {
    "cy:e2e:run": "npm run clear && npm run cy:run && npm run report",
    "cy:open": "cypress open --env START_DOCKER=false --config-file cypress.config.js",
    "cy:run:prod": "cypress run --config-file cypress.config.js",
    "cy:run:debug": "cypress run --env START_DOCKER=false --config-file cypress.config.js",
    "report": "npx mochawesome-merge cypress/reports/mochawesome/*.json -o cypress/reports/mochawesome/merge.json && npx mochawesome-report-generator cypress/reports/mochawesome/merge.json -o cypress/reports/html ",
    "clear": "rm -r cypress/reports/**  || true",
    "snapshot": "npx cypress run --env updateSnapshots=true"
    },
  "keywords": [
    "uia"
  ],
  "author": "Yiming Zhu",
  "license": "ISC",
  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^2.40.2",
    "allure-commandline": "^2.33.0",
    "cypress": "^12.14.0",
    "cypress-dotenv": "^3.0.1",
    "cypress-visual-regression": "^5.3.0",
    "cypress-wait-until": "^3.0.2",
    "junit": "^1.4.9",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.2.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "axios": "^1.8.3",
    "cypress-image-diff-js": "^1.32.0",
    "json-diff": "^1.0.6"
  }
}
